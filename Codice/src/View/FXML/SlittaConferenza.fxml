<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="144.0" prefWidth="364.0" stylesheets="@../CSS/NewStyle.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="giorniTextField" layoutX="14.0" prefHeight="47.0" prefWidth="369.0" text="Di quanti giorni bisogna slittare la conferenza?" textFill="#eceff4">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
      <ButtonBar layoutX="157.0" layoutY="90.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="annullaButton" mnemonicParsing="false" onAction="#annullaButtonOnAction" text="Annulla" />
            <Button fx:id="okButton" mnemonicParsing="false" onAction="#okButtonOnAction" text="Ok" />
        </buttons>
      </ButtonBar>
      <TextField fx:id="giorniTF" layoutX="14.0" layoutY="49.0" prefHeight="24.0" prefWidth="343.0" promptText="Inserire un numero" />
   </children>
</AnchorPane>
