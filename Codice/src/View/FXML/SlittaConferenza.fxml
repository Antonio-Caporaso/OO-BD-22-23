<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="144.0" prefWidth="544.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="14.0" layoutY="13.0" prefHeight="47.0" prefWidth="369.0" text="Di quanti giorni bisogna slittare la conferenza?">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
      <Spinner fx:id="giorniSpinner" editable="true" layoutX="373.0" layoutY="25.0" />
      <ButtonBar layoutX="330.0" layoutY="90.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="annullaButton" mnemonicParsing="false" onAction="#annullaButtonOnAction" text="Annulla" />
            <Button fx:id="okButton" mnemonicParsing="false" onAction="#okButtonOnAction" text="Ok" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
