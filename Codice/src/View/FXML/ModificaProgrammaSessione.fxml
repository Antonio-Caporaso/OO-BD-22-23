<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="932.0" scaleShape="false" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="90.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="932.0" />
      <Label layoutX="14.0" layoutY="27.0" text="Gestione del programma" textFill="WHITE">
         <font>
            <Font name="System Bold" size="30.0" />
         </font></Label>
      <Button fx:id="fineButton" layoutX="707.0" layoutY="675.0" mnemonicParsing="false" onAction="#fineButtonOnAction" prefHeight="24.0" prefWidth="216.0" text="Fine" />
      <VBox layoutX="707.0" layoutY="99.0" prefHeight="328.0" prefWidth="216.0">
         <children>
            <Button fx:id="addPuntoButton" mnemonicParsing="false" onAction="#addPuntoOnAction" prefHeight="65.0" prefWidth="222.0" text="Aggiungi punto" />
            <Button fx:id="deletePuntoButton" mnemonicParsing="false" onAction="#deletePuntoOnAction" prefHeight="65.0" prefWidth="224.0" text="Rimuovi punto">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="editPuntoOnAction" mnemonicParsing="false" onAction="#editPuntoOnAction" prefHeight="65.0" prefWidth="232.0" text="Modifica punto">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Button>
         </children></VBox>
      <TableView fx:id="programmaTableView" layoutX="13.0" layoutY="99.0" prefHeight="603.0" prefWidth="688.0" stylesheets="@../CSS/TableView.css">
         <columns>
            <TableColumn fx:id="appuntamentoTableColumn" editable="false" maxWidth="256.0" prefWidth="135.0" resizable="false" text="Appuntamento" />
            <TableColumn fx:id="inizioTableColumn" editable="false" maxWidth="209.0" minWidth="0.0" prefWidth="148.0" resizable="false" text="Inizio" />
            <TableColumn fx:id="fineTableColumn" editable="false" maxWidth="209.0" minWidth="0.0" prefWidth="123.0" resizable="false" text="Fine" />
            <TableColumn fx:id="speakerTableColumn" editable="false" maxWidth="409.0" prefWidth="159.0" resizable="false" text="Speaker" />
            <TableColumn fx:id="descrizioneTableColumn" editable="false" maxWidth="500.0" prefWidth="146.0" text="Descrizione" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
