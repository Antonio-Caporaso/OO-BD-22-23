            <Font name="System Bold" size="30.0" />
         </font></Label>
      <Label layoutX="14.0" layoutY="121.0" text="Seleziona sponsor">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="158.0" text="Inserisci contributo">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <ChoiceBox fx:id="sponsorChoice" layoutX="232.0" layoutY="119.0" prefHeight="24.0" prefWidth="475.0" />
      <TextField fx:id="contributoTextField" layoutX="232.0" layoutY="156.0" prefHeight="24.0" prefWidth="118.0" />
      <Button fx:id="inserisciButton" layoutX="722.0" layoutY="119.0" mnemonicParsing="false" onAction="#inserisciSponsorOnAction" prefHeight="61.0" prefWidth="196.0" text="Inserisci" />
