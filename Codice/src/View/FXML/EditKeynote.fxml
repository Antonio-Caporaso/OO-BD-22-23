<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="932.0" scaleShape="false" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="90.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="932.0" />
      <Label layoutX="14.0" layoutY="27.0" text="Modifica keynote speaker" textFill="WHITE">
         <font>
            <Font name="System Bold" size="30.0" />
         </font></Label>
      <Button fx:id="fineButton" layoutX="707.0" layoutY="675.0" mnemonicParsing="false" onAction="#fineButtonOnAction" prefHeight="24.0" prefWidth="190.0" text="Fine" />
      <VBox layoutX="707.0" layoutY="112.0" prefHeight="542.0" prefWidth="190.0">
         <children>
            <Button fx:id="addKeynoteButton" mnemonicParsing="false" onAction="#addKeynoteButtonOnAction" prefHeight="65.0" prefWidth="190.0" text="Aggiungi keynote" />
            <Button fx:id="deleteEventoButton" mnemonicParsing="false" onAction="#deleteEventoOnAction" prefHeight="65.0" prefWidth="190.0" text="Rimuovi keynote">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Button>
         </children></VBox>
      <TableView fx:id="keynoteSpeakerTable" layoutX="27.0" layoutY="112.0" prefHeight="65.0" prefWidth="653.0">
         <columns>
            <TableColumn fx:id="nomeKeynoteColumn" prefWidth="75.0" text="Nome" />
            <TableColumn fx:id="cognomeKeynoteColumn" prefWidth="75.0" text="Cognome" />
            <TableColumn fx:id="istituzioneKeynoteColumn" prefWidth="112.0" text="Istituzione" />
            <TableColumn fx:id="emailKeynoteColumn" prefWidth="129.0" text="Email" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
