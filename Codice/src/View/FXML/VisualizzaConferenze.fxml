<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="932.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="90.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="932.0" />
      <Label layoutX="14.0" layoutY="27.0" text="Ricerca conferenze" textFill="WHITE">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <HBox layoutX="14.0" layoutY="81.0" prefHeight="99.0" prefWidth="913.0">
         <children>
            <VBox prefHeight="99.0" prefWidth="852.0">
               <children>
                  <Label text="Cerca per sede">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font>
                  </Label>
                  <ChoiceBox fx:id="sedeChoice" prefHeight="24.0" prefWidth="717.0">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </ChoiceBox>
               </children>
            </VBox>
            <Button fx:id="cercaPerSedeButton" mnemonicParsing="false" onAction="#cercaPerSedeOnAction" prefHeight="24.0" prefWidth="275.0" text="Cerca">
               <HBox.margin>
                  <Insets right="20.0" top="50.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox layoutX="14.0" layoutY="142.0" prefHeight="99.0" prefWidth="913.0">
         <children>
            <VBox prefHeight="37.0" prefWidth="688.0">
               <children>
                  <Label text="Cerca per data">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font>
                  </Label>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <DatePicker fx:id="dataInizioDP" promptText="Da">
                           <HBox.margin>
                              <Insets top="10.0" />
                           </HBox.margin>
                        </DatePicker>
                        <DatePicker fx:id="dataFineDP" promptText="A">
                           <HBox.margin>
                              <Insets left="30.0" top="10.0" />
                           </HBox.margin>
                        </DatePicker>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <Button fx:id="cercaPerDataButton" mnemonicParsing="false" onAction="#cercaPerDataOnAction" prefHeight="24.0" prefWidth="161.0" text="Cerca">
               <HBox.margin>
                  <Insets left="50.0" right="20.0" top="50.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <TableView fx:id="tableConferenza" layoutX="14.0" layoutY="241.0" prefHeight="468.0" prefWidth="717.0">
        <columns>
          <TableColumn fx:id="nomeConferenzaColumn" editable="false" prefWidth="75.0" text="Nome" />
          <TableColumn fx:id="inizioConferenzaColumn" editable="false" prefWidth="75.0" text="Inizio" />
            <TableColumn fx:id="fineConferenzaColumn" editable="false" prefWidth="75.0" text="Fine" />
            <TableColumn fx:id="descrizioneColumn" editable="false" prefWidth="115.0" text="Descrizione" />
            <TableColumn fx:id="sedeColumn" editable="false" prefWidth="89.0" text="Sede" />
            <TableColumn fx:id="budgetColumn" editable="false" prefWidth="96.0" text="Budget" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="visualizzaButton" layoutX="750.0" layoutY="241.0" mnemonicParsing="false" onAction="#visualizzaButtonOnAction" prefHeight="51.0" prefWidth="158.0" text="Visualizza dettagli conferenza" textAlignment="CENTER" wrapText="true" />
   </children>
</AnchorPane>
